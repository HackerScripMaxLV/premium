   Main:AddSeperator("Mob Aura")

	Main:AddToggle("Auto Farm Mob Aura",_G.AutoMobAura,function(value)
		_G.AutoMobAura = value
        StopTween(_G.AutoMobAura)
		if not _G.AutoMobAura then
			Usefastattack = false
		end
	end)
	do -- Init
		task = task or getrenv().task;
		fastSpawn,fastWait,delay = task.spawn,task.wait,task.delay
	end
	_G.DistanceMobAura = 100
	spawn(function()
		while wait() do
			if _G.AutoMobAura then
				for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
					if _G.AutoMobAura and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position-game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= _G.DistanceMobAura then
						repeat wait()
							EquipWeapon(_G.SelectWeapon)
							PosMonAura = MytoTarget(v.HumanoidRootPart.CFrame)
							    _G.FastAttack = true
							if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
								local args = {
									[1] = "Buso"
								}
								game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
							end
                        MytoTarget(v.HumanoidRootPart.CFrame * CFrame.new(5,30,7))
							Click()
							_G.MagnetMobAura = true
							if delay then
								delay(1,function()
									_G.MagnetMobAura = true
								end)
							end 
						until not _G.AutoMobAura or not v.Parent or v.Humanoid.Health <= 0
						    _G.FastAttack = false
					end
				end
			end
		end
	end)
	spawn(function()
		while true do game:GetService("RunService").RenderStepped:Wait()
			if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
				if _G.MagnetMobAura and _G.AutoMobAura then
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if _G.MagnetMobAura and _G.AutoMobAura and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and (v.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 350 then
							v.HumanoidRootPart.CFrame = PosMon
							v.Humanoid.JumpPower = 0
							v.Humanoid.WalkSpeed = 0
							v.HumanoidRootPart.CanCollide = false
							v.HumanoidRootPart.Size = Vector3.new(80,80,80)
							if v.Humanoid:FindFirstChild("Animator") then
								v.Humanoid.Animator:Destroy()
							end
							sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
							v.Humanoid:ChangeState(11)
						end
					end
				end
			end
		end
	end)

    _G.DistanceMobAura = 100
