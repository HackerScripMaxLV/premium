                NameQuest = "ChocQuest2"
                NameMon = "Sweet Thief"
                CFrameQuest = CFrame.new(151.3131103515625, 24.87554359436035, -12774.681640625)
            elseif MyLevel >= 2375 or SelectMonster == "Candy Rebel [Lv. 2375]" then
